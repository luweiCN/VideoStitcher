{
  "name": "video-stitcher",
  "version": "0.5.13",
  "description": "视频拼接工具 - 将两个视频文件前后拼接合成",
  "author": "Your Name <your.email@example.com>",
  "main": "src/main.js",
  "private": true,
  "publish": {
    "provider": "github",
    "owner": "luweiCN",
    "repo": "VideoStitcher"
  },
  "scripts": {
    "build:preload": "tsc -p tsconfig.preload.json",
    "build:main": "tsc -p tsconfig.main.json",
    "dev:renderer": "vite",
    "dev:electron": "nodemon",
    "dev": "npm run build:preload && npm run build:main && concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "build:renderer": "vite build",
    "build": "npm run build:preload && npm run build:main && npm run build:renderer",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/extract-zip": "^2.0.0",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^30.0.0",
    "electron-builder": "^26.7.0",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "extract-zip": "^2.0.1",
    "ffmpeg-static": "^5.3.0",
    "lucide-react": "^0.562.0",
    "node-machine-id": "^1.1.12",
    "sharp": "^0.33.5"
  }
}
