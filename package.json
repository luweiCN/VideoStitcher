{
  "name": "video-stitcher",
  "version": "0.1.20",
  "description": "视频拼接工具 - 将两个视频文件前后拼接合成",
  "author": "Your Name <your.email@example.com>",
  "main": "src/main.js",
  "private": true,
  "publish": {
    "provider": "github",
    "owner": "luweiCN",
    "repo": "VideoStitcher"
  },
  "scripts": {
    "build:preload": "tsc -p tsconfig.preload.json",
    "dev:renderer": "vite",
    "dev:electron": "nodemon",
    "dev": "npm run build:preload && concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "build:renderer": "vite build",
    "build": "npm run build:preload && npm run build:renderer && electron-builder --win --x64 --publish never",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "npm run build:preload && npm run build:renderer && electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-dmg": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/publisher-github": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "electron": "^30.0.0",
    "electron-builder": "^26.7.0",
    "electron-reloader": "^1.2.3",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.7.3",
    "ffmpeg-static": "^5.3.0",
    "lucide-react": "^0.562.0",
    "sharp": "^0.33.5"
  }
}
